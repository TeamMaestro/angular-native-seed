  "scripts": {
    "prepPhone": "gulp build.Phone",
    "prepTablet": "gulp build.Default",
    "prepCLIPhone": "gulp build.cli.Phone",
    "prepCLITablet": "gulp build.cli.Default",
    "ios": "npm run prepCLITablet && tns run ios",
    "ios.phone": "npm run prepCLIPhone && tns run ios",
    "android": "npm run prepCLITablet && tns run android",
    "android.phone": "npm run prepCLIPhone && tns run android",
    "phone-ios-bundle": "npm run prepPhone && npm run start-ios-bundle",
    "tablet-ios-bundle": "npm run prepTablet && npm run start-ios-bundle",
    "build.phone-ios-bundle": "npm run prepPhone && npm run build-ios-bundle",
    "build.tablet-ios-bundle": "npm run prepTablet && npm run build-ios-bundle",
    "phone-android-bundle": "npm run prepPhone && npm run start-android-bundle",
    "tablet-android-bundle": "npm run prepTablet && npm run start-android-bundle",
    "build.phone-android-bundle": "npm run prepPhone && npm run build-android-bundle",
    "build.tablet-android-bundle": "npm run prepTablet && npm run build-android-bundle",
    "ns-bundle": "ns-bundle",
    "livesync": "gulp tns.Livesync",
    "livesync.phone": "gulp tns.Livesync.Phone",
    "start-android-bundle": "npm run ns-bundle --android --start-app",
    "start-ios-bundle": "npm run ns-bundle --ios --start-app",
    "build-android-bundle": "npm run ns-bundle --android --build-app",
    "build-ios-bundle": "npm run ns-bundle --ios --build-app"
  }
}
